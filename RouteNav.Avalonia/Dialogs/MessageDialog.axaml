<platform:Dialog xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dialogs="clr-namespace:NSE.RouteNav.Dialogs"
        xmlns:platform="clr-namespace:NSE.RouteNav"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NSE.RouteNav.Dialogs.MessageDialog"
        x:DataType="dialogs:MessageDialog">
    <StackPanel HorizontalAlignment="Center">
        <TextBlock HorizontalAlignment="Center" />
        <ContentControl HorizontalAlignment="Center" Content="{CompiledBinding}" ContentTemplate="{CompiledBinding DialogButtonsTemplate}" />
    </StackPanel>
</platform:Dialog>