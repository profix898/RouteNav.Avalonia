<routeNav:Page xmlns="https://github.com/avaloniaui"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:routeNav="clr-namespace:RouteNav.Avalonia;assembly=RouteNav.Avalonia"
               mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
               Title="Page 3"
               x:Class="DemoApp.Pages.Main.MainPage3">
    <StackPanel>
        <TextBlock FontWeight="Bold">Welcome on the Main stack!</TextBlock>
        <TextBlock Margin="0,0,0,10">-> We are on Page 3.</TextBlock>
        <Border Background="#EEEEEE" Margin="0, 10" Padding="10">
            <StackPanel Spacing="10">
                <TextBlock Margin="0,0,0,10" FontWeight="Bold">Some controls</TextBlock>
                <ProgressBar Value="0.5" />
                <CheckBox IsChecked="true" />
                <NumericUpDown Minimum="0" Maximum="100" Increment="10" Value="25" />
                <TimePicker />
            </StackPanel>
        </Border>
    </StackPanel>
</routeNav:Page>