<routeNav:Page xmlns="https://github.com/avaloniaui"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:routing="clr-namespace:RouteNav.Avalonia.Routing;assembly=RouteNav.Avalonia"
               xmlns:routeNav="clr-namespace:RouteNav.Avalonia;assembly=RouteNav.Avalonia"
               mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
               Title="Main Page"
               x:Class="DemoApp.Pages.PageMain">
    <StackPanel>
        <TextBlock Foreground="Blue">Welcome on the MainPage!</TextBlock>
        <routing:RouteButton RoutePath="/main/page1">To Page1</routing:RouteButton>
        <TextBlock Foreground="Gray" Margin="0,20,0,0">Secondary navigation stack</TextBlock>
        <routing:RouteButton RoutePath="/second/">To 2nd Main</routing:RouteButton>
        <routing:RouteButton RoutePath="/second/page2">To 2nd Page2</routing:RouteButton>
    </StackPanel>
</routeNav:Page>